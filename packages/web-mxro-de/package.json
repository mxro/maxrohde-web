{
  "name": "web-mxro-de",
  "version": "0.1.0",
  "license": "",
  "author": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:next": "next build && next export -o webDist/",
    "clean": "rm -rf ./dist ./webDist",
    "compile": "tsc --isolatedModules false -p tsconfig.compile.json",
    "compile:sass": "node-sass src/styles/app.scss src/styles/app.css --source-map src/styles/app.css.map",
    "compile:watch": "tsc --isolatedModules false -p tsconfig.compile.json --watch",
    "compile:watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "deploy": "template-nextjs set-nextjs-env $@ && yarn build && yarn web:build && template deploy $@",
    "infra": "template-nextjs infra",
    "next:tracking:disable": "next telemetry disable",
    "next:watch": "next",
    "prepublishOnly": "yarn run build",
    "start:next": "next start",
    "template": "template-nextjs",
    "test": "jest --config=./jest.config.js --watch",
    "test:ci": "jest --passWithNoTests --config=./jest.config.js",
    "watch": "yarn next:watch",
    "web:build": "yarn build:next",
    "web:watch": "yarn web:build && concurrently \"reload --dir ./webDist --watch-dir ./webDist --port 8071 --browser\" "
  },
  "dependencies": {
    "ignore-loader": "^0.1.2",
    "next": "^13.5.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@goldstack/template-nextjs": "^0.5.18",
    "@goldstack/utils-sh": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/testing-library__react": "^10.2.0",
    "concurrently": "^7.3.0",
    "extracted-loader": "^1.0.7",
    "ignore-loader": "^0.1.2",
    "jest": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.4.0",
    "node-sass": "^7.0.3",
    "nodemon": "^2.0.15",
    "reload": "^3.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
